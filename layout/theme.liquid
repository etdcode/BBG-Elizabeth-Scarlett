$('.faq_new .accordian_item').click(function() {
  if ($(this).hasClass('active')) {
    $(this).removeClass('active');
    $(this).siblings('.ps-relative .text').slideUp(200);
  } else {   
    $('.faq_new .ps-relative .text').slideUp(200);
    $('.faq_new .accordian_item').removeClass('active');
    $(this).addClass('active');
    $(this).siblings('.faq_new .ps-relative .text').slideDown(200);   
  }
});
</script>
  
